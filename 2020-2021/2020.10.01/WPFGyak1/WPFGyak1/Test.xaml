<Window x:Class="WPFGyak1.Test"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFGyak1"
        mc:Ignorable="d"
        Title="Test" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="Text1" HorizontalAlignment="Left" Height="23" Margin="271,189,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="Button_1" Content="Insert" HorizontalAlignment="Left" Margin="418,189,0,0" VerticalAlignment="Top" Width="75" Height="23" Click="Button_1_Click"/>
        <ListBox x:Name="lb1" HorizontalAlignment="Left" Height="129" Margin="271,33,0,0" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="Button_2" Content="Delete" HorizontalAlignment="Left" Margin="418,33,0,0" VerticalAlignment="Top" Width="75" Height="129" Click="Button_2_Click"/>
        <TextBox x:Name="Update_Text" HorizontalAlignment="Left" Height="23" Margin="271,233,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="Update_Button" Content="Update" HorizontalAlignment="Left" Margin="418,233,0,0" VerticalAlignment="Top" Width="75" Height="23" Click="Update_Button_Click"/>

    </Grid>
</Window>
