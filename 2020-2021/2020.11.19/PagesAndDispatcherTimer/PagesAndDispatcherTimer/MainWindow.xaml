<Window x:Class="PagesAndDispatcherTimer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PagesAndDispatcherTimer"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <Grid>
        <DockPanel>
            <Label x:Name="ClockLabel" DockPanel.Dock="Top" Content="" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"></Label>
        <Frame x:Name="MainFrame" Content="" NavigationUIVisibility="Hidden"/>
        </DockPanel>
    </Grid>
</Window>
